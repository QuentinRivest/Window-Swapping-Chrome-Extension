{
  "name" : "Window Swapper",
  "description" : "A Chrome Extension for saving and managing your browser windows, and even swapping your current window for one you have saved.",
  "version" : "1.0",
  "manifest_version" : 3,
  "permissions" : [
    "storage",
    "tabs"
  ],
  "icons" : {
    "16" : "images/swap-icon.png",
    "32" : "images/swap-icon.png",
    "48" : "images/swap-icon.png",
    "128" : "images/swap-icon.png"
  },
  "action" : {
    "default_popup" : "pages/popup.html",
    "default_icon" : "images/swap-icon.png"
  },
  "background" : {
    "service_worker" : "js/service-worker.js"
  }
}
